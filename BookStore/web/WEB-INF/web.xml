<web-app xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
		 version="3.0" xmlns:xsi="http://java.sun.com/xml/ns/j2ee">

<servlet>
	<servlet-name>editarPersonal</servlet-name>
	<servlet-class>com.bookstore.controlador.EditarInformacionPersonal
	</servlet-class>
</servlet>
<servlet>
	<servlet-name>editarCuenta</servlet-name>
	<servlet-class>com.bookstore.controlador.EditarCuenta
	</servlet-class>
</servlet>


<servlet-mapping>
	<servlet-name>editarCuenta</servlet-name>
	<url-pattern>/EditarCuenta</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>editarPersonal</servlet-name>
	<url-pattern>/EditarInformacionPersonal</url-pattern>
</servlet-mapping>

<session-config>
<session-timeout>30</session-timeout>
</session-config>
<welcome-file-list>
<welcome-file>index.jsp</welcome-file>
</welcome-file-list>
</web-app>